function addRatingListener(){$(".rating").raty({cancel:!0,half:!1,click:function(a){var b=$(this).attr("data-tutorial");$.ajax({type:"POST",url:"/ajax/students/insertLessonSetting",data:{tutorial:b,score:a},dataType:"json",success:function(a){1==a.sessionexpired&&showSessionExpired()}})},path:"/assets/css/raty",score:function(){return $(this).attr("data-score")}})}function addFavouriteListener(){$(".favourite").on("click",function(){var a=$(this),b=a.attr("data-tutorial"),c=a.hasClass("favourite-on")?"Y":"N";$.ajax({type:"POST",url:"/ajax/students/updateFavourite",data:{tutorial:b,favourite:c},dataType:"json",success:function(b){1==b.sessionexpired?showSessionExpired():"Y"==c?a.removeClass("fa-heart favourite-on").addClass("fa-heart-o favourite-off"):a.removeClass("fa-heart-o favourite-off").addClass("fa-heart favourite-on")}})})}